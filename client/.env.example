# TechGadgets Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_URL=http://localhost:5002
REACT_APP_NAME=TechGadgets

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_DEBUG_MODE=true
REACT_APP_ENABLE_PWA=false

# Third-party Services (Optional)
# REACT_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X
# REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
# REACT_APP_SENTRY_DSN=your_sentry_dsn_for_error_tracking

# Social Media Links (Optional)
# REACT_APP_FACEBOOK_URL=https://facebook.com/techgadgets
# REACT_APP_TWITTER_URL=https://twitter.com/techgadgets
# REACT_APP_INSTAGRAM_URL=https://instagram.com/techgadgets

# Performance Monitoring (Optional)
# REACT_APP_ENABLE_WEB_VITALS=true

# Build Configuration (Optional)
# GENERATE_SOURCEMAP=false
# INLINE_RUNTIME_CHUNK=false
